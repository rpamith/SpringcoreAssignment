<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">
           
           
           
<bean id="question" class="springcore2.Question">  
	<constructor-arg index="0" value="1"/>  
	<constructor-arg index="1" value="Name any 2 design pattern ?"/>  
	<constructor-arg index="2">  
	<list>  
		 <value>SingeltonDP</value>
		 <value>Factory DP</value>
	</list>  
	</constructor-arg> 

	<constructor-arg index="3">  
	<set>  
		 <value>SingeltonDP</value>
		 <value>Factory DP</value>
	</set>  
	</constructor-arg> 

<constructor-arg index="4">  
<map>  
<entry key="1"  value="SingeltonDP"></entry>  
<entry key="2" value="Factory DP"></entry>  
</map>  
</constructor-arg>  
 
</bean>   
           
</beans>